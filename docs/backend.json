{
  "entities": {
    "AdminUser": {
      "$schema": "http://json-schema.org/draft-07/schema#",
      "title": "AdminUser",
      "type": "object",
      "description": "Represents an authorized administrator account on the Temmy Realty Hub platform, managing property listings and rental/sales processes.",
      "properties": {
        "id": {
          "type": "string",
          "description": "Unique identifier for the AdminUser entity."
        },
        "fullName": {
          "type": "string",
          "description": "The full name of the administrator."
        },
        "email": {
          "type": "string",
          "description": "The email address of the administrator, used for login and contact.",
          "format": "email"
        },
        "role": {
          "type": "string",
          "description": "The administrative role assigned to the user, defining their permissions and access level."
        },
        "registrationDate": {
          "type": "string",
          "description": "The timestamp when the admin user account was registered.",
          "format": "date-time"
        },
        "lastLoginDate": {
          "type": "string",
          "description": "The timestamp of the administrator's last successful login.",
          "format": "date-time"
        }
      },
      "required": [
        "id",
        "fullName",
        "email",
        "role",
        "registrationDate"
      ]
    },
    "PublicUser": {
      "$schema": "http://json-schema.org/draft-07/schema#",
      "title": "PublicUser",
      "type": "object",
      "description": "Represents a regular customer (buyer or renter) on the platform.",
      "properties": {
        "id": {
          "type": "string",
          "description": "The unique Firebase Auth UID of the user."
        },
        "fullName": {
          "type": "string",
          "description": "The user's full name."
        },
        "email": {
          "type": "string",
          "format": "email",
          "description": "The user's email address."
        },
        "createdAt": {
          "type": "string",
          "format": "date-time",
          "description": "Timestamp of account creation."
        }
      },
      "required": ["id", "fullName", "email", "createdAt"]
    },
    "PropertyListing": {
      "$schema": "http://json-schema.org/draft-07/schema#",
      "title": "PropertyListing",
      "type": "object",
      "description": "Represents a single real estate property available for either sale or rent on the Temmy Realty Hub platform.",
      "properties": {
        "id": {
          "type": "string",
          "description": "Unique identifier for the PropertyListing entity."
        },
        "adminId": {
          "type": "string",
          "description": "Reference to the AdminUser who created or last updated this property listing."
        },
        "title": {
          "type": "string",
          "description": "A concise title for the property listing."
        },
        "description": {
          "type": "string",
          "description": "A detailed description of the property."
        },
        "listingType": {
          "type": "string",
          "description": "Specifies whether the property is for 'Sale' or 'Rent'."
        },
        "propertyType": {
          "type": "string",
          "description": "The specific type of property."
        },
        "address": {
          "type": "string",
          "description": "The street address of the property."
        },
        "city": {
          "type": "string",
          "description": "The city where the property is located."
        },
        "state": {
          "type": "string",
          "description": "The state where the property is located."
        },
        "zipCode": {
          "type": "string",
          "description": "The ZIP code of the property's location."
        },
        "price": {
          "type": "number",
          "description": "The selling price or monthly rental price."
        },
        "currency": {
          "type": "string",
          "description": "The currency of the price."
        },
        "numBedrooms": {
          "type": "number",
          "description": "Total number of bedrooms."
        },
        "numBathrooms": {
          "type": "number",
          "description": "Total number of bathrooms."
        },
        "squareFootage": {
          "type": "number",
          "description": "Total interior living area in square feet."
        },
        "lotSize": {
          "type": "number",
          "description": "Total area of the property's lot in square feet."
        },
        "yearBuilt": {
          "type": "number",
          "description": "Year of construction."
        },
        "amenities": {
          "type": "array",
          "description": "List of amenities.",
          "items": {
            "type": "string"
          }
        },
        "imageUrls": {
          "type": "array",
          "description": "Array of URLs pointing to high-quality images.",
          "items": {
            "type": "string"
          }
        },
        "status": {
          "type": "string",
          "description": "Availability status (e.g., 'Available', 'Sold')."
        },
        "listingDate": {
          "type": "string",
          "description": "Timestamp of listing creation.",
          "format": "date-time"
        },
        "lastUpdatedDate": {
          "type": "string",
          "description": "Timestamp of last update.",
          "format": "date-time"
        }
      },
      "required": [
        "id",
        "adminId",
        "title",
        "description",
        "listingType",
        "propertyType",
        "address",
        "city",
        "state",
        "zipCode",
        "price",
        "currency",
        "numBedrooms",
        "numBathrooms",
        "squareFootage",
        "amenities",
        "imageUrls",
        "status",
        "listingDate",
        "lastUpdatedDate"
      ]
    }
  },
  "auth": {
    "providers": [
      "password",
      "anonymous"
    ]
  },
  "firestore": {
    "structure": [
      {
        "path": "/admin_users/{adminId}",
        "definition": {
          "entityName": "AdminUser",
          "schema": {
            "$ref": "#/backend/entities/AdminUser"
          },
          "description": "Stores profile information for authorized administrators.",
          "params": [
            {
              "name": "adminId",
              "description": "The UID of the administrator."
            }
          ]
        }
      },
      {
        "path": "/users/{userId}",
        "definition": {
          "entityName": "PublicUser",
          "schema": {
            "$ref": "#/backend/entities/PublicUser"
          },
          "description": "Stores profile information for regular users.",
          "params": [
            {
              "name": "userId",
              "description": "The UID of the user."
            }
          ]
        }
      },
      {
        "path": "/property_listings/{propertyId}",
        "definition": {
          "entityName": "PropertyListing",
          "schema": {
            "$ref": "#/backend/entities/PropertyListing"
          },
          "description": "Contains all property listings available for sale or rent.",
          "params": [
            {
              "name": "propertyId",
              "description": "The unique identifier for a specific property listing."
            }
          ]
        }
      }
    ]
  }
}